<AnchorPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: #E9EEF3;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.animelist.Controleur.CatalogueControlleur">
